{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "indent": ["error", 2],
    "no-unused-vars": "off",
    "semi": ["error", "never"]
  },
  "parser": "@typescript-eslint/parser",
  "env": {
    "node": true
  }
}